{
  "name": "base16-css-vars",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#000005",
    "editor.selectionBackground": "#000002",
    "editor.lineHighlightBackground": "#000001",
    "editorCursor.foreground": "#000005",
    "editorGutter.modifiedBackground": "#000009",
    "editorGutter.addedBackground": "#00000B",
    "editorGutter.deletedBackground": "#000008"
  },
  "tokenColors": [
    {
      "name": "Classes",
      "scope": "meta.class",
      "settings": {
        "foreground": "#000005"
      }
    },
    {
      "name": "Text",
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#000005"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#000003",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.variable",
        "punctuation.definition.parameters",
        "punctuation.definition.array",
        "punctuation.definition.block",
        "punctuation.separator",
        "punctuation.accessor",
        "meta.template.expression",
        "meta.array.literal",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#000005"
      }
    },
    {
      "name": "Delimiters",
      "scope": "none",
      "settings": {
        "foreground": "#000005"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#000005"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#00000E"
      }
    },
    {
      "name": "Decorator",
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#000005"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.type.property-name",
        "entity.name.variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#000008"
      }
    },
    {
      "name": "Object Property Names",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#000005"
      }
    },
    {
      "name": "Classes",
      "scope": ["support.class", "entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#00000A"
      }
    },
    {
      "name": "Named Type",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": {
        "foreground": "#00000A"
      }
    },
    {
      "name": "Support Constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#00000A"
      }
    },
    {
      "name": "Primitive/Builtin Type",
      "scope": [
        "support.type.primitive",
        "support.type.builtin",
        "keyword.type"
      ],
      "settings": {
        "foreground": "#000005"
      }
    },
    {
      "name": "Method Definition",
      "scope": "meta.definition.method",
      "settings": {
        "foreground": "#000005"
      }
    },
    {
      "name": "Methods",
      "scope": [
        "meta.function-call",
        "meta.require",
        "variable.language.super",
        "keyword.other.special-method",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#00000D"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#00000E"
      }
    },
    {
      "name": "Support",
      "scope": "support.function",
      "settings": {
        "foreground": "#00000C"
      }
    },
    {
      "name": "Strings, Inherited Class",
      "scope": [
        "string",
        "punctuation.definition.string",
        "constant.other.symbol",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#00000B"
      }
    },
    {
      "name": "Integers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#000009"
      }
    },
    {
      "name": "Floats",
      "scope": "none",
      "settings": {
        "foreground": "#000009"
      }
    },
    {
      "name": "Boolean",
      "scope": "none",
      "settings": {
        "foreground": "#000009"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#000009"
      }
    },
    {
      "name": "Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#000009",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#000008"
      }
    },
    {
      "name": "Attribute IDs",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#00000D"
      }
    },
    {
      "name": "Selector",
      "scope": "meta.selector",
      "settings": {
        "foreground": "#00000E"
      }
    },

    {
      "name": "Values",
      "scope": "none",
      "settings": {
        "foreground": "#000009"
      }
    },
    {
      "name": "CSS Property Name",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#000005"
      }
    },
    {
      "name": "CSS Property Value",
      "scope": "support.constant.property-value.css",
      "settings": {
        "foreground": "#000009"
      }
    },
    {
      "name": "Headings",
      "scope": [
        "markup.heading punctuation.definition.heading",
        "entity.name.section"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#000008"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#000009"
      }
    },
    {
      "name": "Bold",
      "scope": "punctuation.definition.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#000009",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": "punctuation.definition.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#00000E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#00000B"
      }
    },
    {
      "name": "Link",
      "scope": ["meta.link", "string.other.link"],
      "settings": {
        "foreground": "#00000D"
      }
    },
    {
      "name": "Link",
      "scope": "string.other.link.title",
      "settings": {
        "foreground": "#00000C"
      }
    },
    {
      "name": "Lists",
      "scope": "beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#000008"
      }
    },
    {
      "name": "Quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#000003"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "foreground": "#000005"
      }
    },
    {
      "name": "Inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#00000B"
      }
    },
    {
      "name": "Deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#000008"
      }
    },
    {
      "name": "Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#00000E"
      }
    },
    {
      "name": "Colors",
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#00000C"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#00000C"
      }
    },
    {
      "name": "Escape Characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#00000C"
      }
    },
    {
      "name": "Embedded",
      "scope": ["punctuation.section.embedded", "variable.interpolation"],
      "settings": {
        "foreground": "#00000F"
      }
    },
    {
      "name": "Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#000007"
      }
    },
    {
      "name": "Broken",
      "scope": "invalid.broken",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#000007"
      }
    },
    {
      "name": "Unimplemented",
      "scope": "invalid.unimplemented",
      "settings": {
        "foreground": "#000007"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    }
  ]
}
